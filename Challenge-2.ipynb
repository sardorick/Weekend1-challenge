    "\n",
    "def reverse_pyramid(num):\n",
    "    i = num\n",
    "    while (i <= num):\n",
    "        print('#' * i)\n",
    "        i -= 1\n",
    "        if i == 0:    \n",
    "            break\n",
    "    return reverse_pyramid   \n",
    "    \n",
