   "source": [
    "def print_pyramid(num):\n",
    "    i = 1\n",
    "    while (i <= num):\n",
    "        print('#' * i)\n",
    "        i += 1\n",
    "    return print_pyramid    \n",
    "    \n",
    "    \n",
    "\n",
    "print(print_pyramid(5))       "
